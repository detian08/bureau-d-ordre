<?xml version="1.0" encoding="utf-8"?>

<openerp>
    <data>
        <record model="ir.sequence.type" id="courrier_code_sequence">
            <field name="name">Code sequence courrier</field>
            <field name="code">document.courrier</field>
        </record>

        <record model="ir.sequence" id="courrier_code_sequencee">
            <field name="name">Sequence courrier</field>
            <field name="code">document.courrier</field>
            <field name="padding">4</field>
            <field name="suffix">-%(year)s</field>
            <field eval="1" name="number_increment"/>
        </record>
    </data>
</openerp>